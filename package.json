{
  "name": "ember-creditcard",
  "version": "0.1.9",
  "description": "Client side credit card type detection and validation for Ember.js apps",
  "scripts": {
    "build": "ember build",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each"
  },
  "engines": {
    "node": "6.* || 8.* || >= 10.*"
  },
  "devDependencies": {
    "@commitlint/cli": "7.6.1",
    "@commitlint/config-conventional": "7.6.0",
    "@commitlint/travis-cli": "7.6.1",
    "@ember/optional-features": "0.7.0",
    "@mike-north/js-lib-renovate-config": "1.3.1",
    "@mike-north/js-lib-semantic-release-config": "1.0.1",
    "broccoli-asset-rev": "3.0.0",
    "ember-cli": "5.6.0",
    "ember-cli-dependency-checker": "3.3.2",
    "ember-cli-eslint": "5.1.0",
    "ember-cli-htmlbars-inline-precompile": "3.0.2",
    "ember-cli-inject-live-reload": "2.1.0",
    "ember-cli-shims": "1.2.0",
    "ember-cli-template-lint": "2.0.2",
    "ember-cli-uglify": "3.0.0",
    "ember-code-snippet": "2.4.2",
    "ember-disable-prototype-extensions": "1.1.3",
    "ember-export-application-global": "2.0.1",
    "ember-load-initializers": "2.1.2",
    "ember-maybe-import-regenerator": "0.1.6",
    "ember-qunit": "8.0.1",
    "ember-resolver": "13.0.0",
    "ember-source": "5.7.0",
    "ember-source-channel-url": "1.2.0",
    "ember-try": "1.4.0",
    "eslint-plugin-ember": "12.2.0",
    "eslint-plugin-node": "11.1.0",
    "husky": "1.3.1",
    "loader.js": "4.7.0",
    "qunit-dom": "3.0.0"
  },
  "keywords": [
    "ember-addon",
    "creditcard"
  ],
  "repository": "https://github.com/mike-north/ember-creditcard",
  "license": "MIT",
  "author": "Mike North <michael.l.north@gmail.com> (http://mike.works)",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "dependencies": {
    "ember-cli-babel": "^8.0.0",
    "ember-cli-htmlbars": "^6.0.0"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "http://mike-north.github.io/ember-creditcard/"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "./node_modules/.bin/commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "release": {
    "extends": "@mike-north/js-lib-semantic-release-config"
  },
  "toolchain": {
    "node": "10.13.0",
    "yarn": "1.10.0"
  }
}
